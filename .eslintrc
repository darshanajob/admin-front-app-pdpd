{
  "rules": {
    "no-undef": "off",
    "react/prop-types" : "off",
    "react/no-unescaped-entities": 0
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false,
      "modules": false,
      "experimentalObjectRestSpread": false
    }
  }
}
